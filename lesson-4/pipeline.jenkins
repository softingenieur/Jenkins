pipeline {
    agent any
    stages {
        stage('Docker version') {
            steps {
                sh 'echo "Docker path:" $(which docker)'
                sh 'docker --version'
            }
        }
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/softingenieur//lesson-4/Jenkins.git'
                sh 'pwd'
                sh 'ls -la'
            }
        }
    }
}
