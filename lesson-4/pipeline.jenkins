pipeline {
    agent { label 'ubuntu' }

    stages {
        stage('Docker version') {
            steps {
                sh "echo $USER"
                sh 'docker version'
            }
        }
        stage('Checkout') {
            steps{
                git branch: 'main',
                    url: 'https://github.com/softingenieur/Jenkins.git'   

                 sh 'pwd'
                 sh 'ls -la'        
                }
        }
    }
}